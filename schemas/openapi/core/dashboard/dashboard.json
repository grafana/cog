          "repeatDirection": {
            "description": "Direction to repeat in if 'repeat' is set.\n\"h\" for horizontal, \"v\" for vertical.\nTODO this is probably optional",
            "type": "string",
            "enum": [
              "h",
              "v"
            ],
            "default": "h"
